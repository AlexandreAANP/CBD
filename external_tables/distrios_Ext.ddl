CREATE TABLE Distrito_ext (
    V_distrito VARCHAR2(20 CHAR)
)
ORGANIZATION EXTERNAL
(
    TYPE ORACLE_LOADER
    DEFAULT DIRECTORY def_dir1
    ACCESS PARAMETERS
    (RECORDS DELIMITED BY NEWLINE
        FIELDS(
            V_distrito CHAR(20)
        ))
        LOCATION('Distritos')-- ficheiro que est√° na pasta external tables
)REJECT LIMIT UNLIMITED;

drop table Distrito_ext;
SELECT * FROM distrito_ext;


